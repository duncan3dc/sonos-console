{
    "name": "duncan3dc/sonos-console",
    "description": "An application to control your Sonos network from the command line",
    "keywords": ["sonos", "upnp", "streaming"],
    "homepage": "https://github.com/duncan3dc/sonos-console",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Craig Duncan",
        "email": "git@duncanc.co.uk",
        "homepage": "https://github.com/duncan3dc",
        "role": "Developer"
    }],
    "require": {
        "duncan3dc/console": "^1.1",
        "duncan3dc/sonos": "^2.0",
        "php": "^7.1"
    },
    "require-dev": {
        "maglnet/composer-require-checker": "^1.0",
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {"duncan3dc\\Sonos\\Console\\": "src/"}
    },
    "autoload-dev": {
